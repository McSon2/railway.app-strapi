      "required": false,
      "allowedTypes": ["images"]
    },
    "canonicalURL": {
      "type": "string"
    },
    "noIndex": {
      "type": "boolean",
      "default": false
    },
    "structuredData": {
      "type": "json"
    }
  }
}
